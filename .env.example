# Copy to .env and fill real values; never commit .env
SQL_SERVER=DESKTOP-XXXXX\SQLEXPRESS
SQL_DATABASE=CreditCardData
SQL_DRIVER=ODBC Driver 17 for SQL Server
SQL_TRUSTED_CONNECTION=yes
CONNECTION_STRING="mssql+pyodbc://%(SQL_SERVER)s/%(SQL_DATABASE)s?driver=%(SQL_DRIVER)s&trusted_connection=%(SQL_TRUSTED_CONNECTION)s"
